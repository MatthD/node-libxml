{
  "name": "node-libxml",
  "version": "3.2.5",
  "description": "Check Wellformed, DTD validity & get Xpath from pure node addons",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jupitex/node-libxml.git"
  },
  "scripts": {
    "test": "mocha",
    "install": "node-pre-gyp install --fallback-to-build",
    "build": "node-pre-gyp build",
    "prebuild": "node-pre-gyp configure --enable-build-type=debug"
  },
  "author": "MatthD",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.3.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nan": "^2.14.0",
    "node-gyp": "5.0.3"
  },
  "binary": {
    "module_name": "xml",
    "module_path": "./build/Release/",
    "remote_path": "{version}",
    "host": "https://github.com/jupitex/node-libxml/releases/download/"
  },
  "gypfile": true,
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "node-pre-gyp": "0.6.38"
  }
}
